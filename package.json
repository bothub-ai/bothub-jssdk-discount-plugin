{
  "name": "bothub-discount-plugin",
  "version": "1.0.0",
  "description": "bothub discount plugin",
  "main": "index.js",
  "keywords": [
    "shopify",
    "remarket",
    "bothub"
  ],
  "author": "wangqian@bothub.ai",
  "license": "ISC",
  "homepage": "https://github.com/bothub-ai/shopify-remarket#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bothub-ai/shopify-remarket.git"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --optimize-minimize"
  },
  "dependencies": {
    "vue": "^2.5.13",
    "vue-loader": "^14.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-component": "^1.1.0",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-plugin-vue-jsx-sync": "0.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-vue": "^4.2.2",
    "less": "^3.0.0",
    "less-loader": "^4.0.5",
    "style-loader": "^0.20.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "modules": false,
          "targets": {
            "browsers": [
              "last 2 versions",
              "ie >= 7"
            ]
          }
        }
      ]
    ],
    "plugins": [
      "transform-object-rest-spread",
      "syntax-dynamic-import",
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential"
    ],
    "parserOptions": {
      "ecmaVersion": 2017
    },
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "window": true,
      "global": true,
      "$ENV": true,
      "BOTHUB": true
    },
    "rules": {
      "semi": [
        0
      ],
      "comma-dangle": [
        0
      ],
      "global-require": [
        0
      ],
      "no-param-reassign": [
        0
      ],
      "no-unused-expressions": [
        0
      ],
      "no-console": [
        0
      ],
      "no-unexpected-multiline": [
        0
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "webpack.config.js"
        }
      }
    }
  }
}
